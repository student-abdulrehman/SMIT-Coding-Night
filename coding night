        "import sys\n",
        "import importlib\n",
        "\n",
        "# Remove existing modules from sys.modules to force a fresh import\n",
        "# This is a common workaround in notebooks when files are modified on disk\n",
        "for module_name in ['services.storage_service', 'services.student_manager', 'models.student',\n",
        "                    'ui.add_student', 'ui.update_student', 'ui.delete_student',\n",
        "                    'ui.list_students', 'ui.search_students', 'ui.fees_attendance']:\n",
        "    if module_name in sys.modules:\n",
        "        del sys.modules[module_name]\n",
        "\n",
        "import pandas as pd\n",
