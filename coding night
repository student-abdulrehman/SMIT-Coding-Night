        "class Student:\n",
        "    def __init__(self, student_id, name, age, grade, gpa):\n",
        "        self.student_id = str(student_id)\n",
        "        self.name = name\n",
        "        self.age = int(age)\n",
        "        self.grade = grade\n",
        "        self.gpa = float(gpa)\n",
        "\n",
        "    def to_dict(self):\n",
        "        \"\"\"Converts object to dictionary for JSON storage.\"\"\"\n",
        "        return vars(self)\n"
      ],
      "metadata": {
        "id": "7kOonQbvFMVD"
