      "source": [
        "!mkdir -p services models ui\n",
        "!touch services/__init__.py models/__init__.py ui/__init__.py\n",
        "\n",
        "file_contents = {\n",
        "    \"services/storage_service.py\": \"\"\"\n",
        "import pandas as pd\n",
        "import json\n",
        "import os\n",
        "\n",
        "class StorageService:\n",
        "    def __init__(self, file_path='students.json'):\n",
        "        self.file_path = file_path\n",
        "        self._ensure_file_exists()\n",
        "\n",
        "    def _ensure_file_exists(self):\n",
        "        if not os.path.exists(self.file_path):\n",
        "            with open(self.file_path, 'w') as f:\n",
        "                json.dump([], f)\n",
        "\n",
        "    def load_data(self):\n",
        "        with open(self.file_path, 'r') as f:\n",
        "            data = json.load(f)\n",
        "        return data\n",
        "\n",
        "    def save_data(self, data):\n",
        "        with open(self.file_path, 'w') as f:\n",
        "            json.dump(data, f, indent=4)\n",
        "\"\"\",\n",
        "    \"services/student_manager.py\": \"\"\"\n",
        "from models.student import Student\n",
        "from services.storage_service import StorageService\n",
        "\n",
        "class StudentManager:\n",
        "    def __init__(self, storage_service: StorageService):\n",
        "        self.storage = storage_service\n",
        "        self.students = self._load_students()\n",
        "\n",
        "    def _load_students(self):\n",
        "        data = self.storage.load_data()\n",
        "        return {s['id']: Student.from_dict(s) for s in data}\n",
        "\n",
        "    def _save_students(self):\n",
        "        data = [s.to_dict() for s in self.students.values()]\n",
        "        self.storage.save_data(data)\n",
        "\n",
        "    def add_student(self, student: Student):\n",
        "        if student.id in self.students:\n",
        "            return False\n",
        "        self.students[student.id] = student\n",
        "        self._save_students()\n",
        "        return True\n",
        "\n",
        "    def update_student(self, student_id, **kwargs):\n",
        "        student = self.students.get(student_id)\n",
        "        if not student:\n",
        "            return False\n",
        "        for key, value in kwargs.items():\n",
        "            if hasattr(student, key):\n",
        "                setattr(student, key, value)\n",
        "        self._save_students()\n",
        "        return True\n",
        "\n",
        "    def delete_student(self, student_id):\n",
        "        if student_id in self.students:\n",
        "            del self.students[student_id]\n",
        "            self._save_students()\n",
        "            return True\n",
        "        return False\n",
        "\n",
        "    def get_student(self, student_id):\n",
        "        return self.students.get(student_id)\n",
        "\n",
        "    def list_students(self):\n",
        "        return list(self.students.values())\n",
        "\n",
        "    def search(self, **kwargs):\n",
        "        results = []\n",
        "        for student in self.students.values():\n",
        "            match = True\n",
        "            for key, value in kwargs.items():\n",
        "                if hasattr(student, key) and str(getattr(student, key)).lower().find(str(value).lower()) == -1:\n",
        "                    match = False\n",
        "                    break\n",
        "            if match:\n",
        "                results.append(student)\n",
        "        return results\n",
        "\n",
        "    def add_payment(self, student_id, amount, method, date):\n",
        "        student = self.students.get(student_id)\n",
        "        if student:\n",
        "            student.amount_paid += amount\n",
        "            student.payments.append({'amount': amount, 'method': method, 'date': date})\n",
        "            self._save_students()\n",
        "            return True\n",
        "        return False\n",
        "\n",
        "    def mark_attendance(self, student_id, month_year, day, status):\n",
        "        student = self.students.get(student_id)\n",
        "        if student:\n",
        "            if month_year not in student.attendance:\n",
        "                student.attendance[month_year] = {}\n",
        "            student.attendance[month_year][str(day)] = status\n",
        "            self._save_students()\n",
        "            return True\n",
        "        return False\n",
        "\n",
        "    def get_month_attendance(self, student_id, month_year):\n",
        "        student = self.students.get(student_id)\n",
        "        if student and month_year in student.attendance:\n",
        "            return student.attendance[month_year]\n",
        "        return {}\n",
        "\"\"\",\n",
        "    \"models/student.py\": \"\"\"\n",
        "import uuid\n",
        "\n",
        "class Student:\n",
        "    def __init__(self, student_id, name, age, grade, performance, total_fee=5000, amount_paid=0, payments=None, attendance=None):\n",
        "        self.id = student_id\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "        self.grade = grade\n",
        "        self.performance = performance\n",
        "        self.total_fee = total_fee\n",
        "        self.amount_paid = amount_paid\n",
        "        self.payments = payments if payments is not None else []\n",
        "        self.attendance = attendance if attendance is not None else {}\n",
        "\n",
        "    @classmethod\n",
        "    def create(cls, name, age, grade, performance, total_fee=5000, amount_paid=0):\n",
        "        student_id = str(uuid.uuid4())\n",
        "        return cls(student_id, name, age, grade, performance, total_fee, amount_paid)\n",
        "\n",
        "    @classmethod\n",
        "    def from_dict(cls, data):\n",
        "        return cls(\n",
        "            data['id'],\n",
        "            data['name'],\n",
        "            data['age'],\n",
        "            data['grade'],\n",
        "            data['performance'],\n",
        "            data.get('total_fee', 5000),\n",
        "            data.get('amount_paid', 0),\n",
        "            data.get('payments', []),\n",
        "            data.get('attendance', {})\n",
        "        )\n",
        "\n",
        "    def to_dict(self):\n",
        "        return {\n",
        "            'id': self.id,\n",
        "            'name': self.name,\n",
        "            'age': self.age,\n",
        "            'grade': self.grade,\n",
        "            'performance': self.performance,\n",
        "            'total_fee': self.total_fee,\n",
        "            'amount_paid': self.amount_paid,\n",
        "            'payments': self.payments,\n",
        "            'attendance': self.attendance\n",
        "        }\n",
        "\n",
        "    def __repr__(self):\n",
        "        return f\"Student(ID: {self.id[:4]}, Name: {self.name}, Grade: {self.grade})\"\n",
        "\"\"\",\n",
        "    \"ui/add_student.py\": \"\"\"\n",
        "import streamlit as st\n",
        "def show_add_student(manager):\n",
        "    st.write(\"Add Student UI (Placeholder)\")\n",
        "\"\"\",\n",
        "    \"ui/update_student.py\": \"\"\"\n",
        "import streamlit as st\n",
        "def show_update_student(manager):\n",
        "    st.write(\"Update Student UI (Placeholder)\")\n",
        "\"\"\",\n",
        "    \"ui/delete_student.py\": \"\"\"\n",
        "import streamlit as st\n",
        "def show_delete_student(manager):\n",
        "    st.write(\"Delete Student UI (Placeholder)\")\n",
        "\"\"\",\n",
        "    \"ui/list_students.py\": \"\"\"\n",
        "import streamlit as st\n",
        "def show_list_students(manager):\n",
        "    st.write(\"List Students UI (Placeholder)\")\n",
        "\"\"\",\n",
        "    \"ui/search_students.py\": \"\"\"\n",
        "import streamlit as st\n",
        "def show_search_students(manager):\n",
        "    st.write(\"Search Students UI (Placeholder)\")\n",
        "\"\"\",\n",
        "    \"ui/fees_attendance.py\": \"\"\"\n",
        "import streamlit as st\n",
        "def show_fees_and_attendance(manager):\n",
        "    st.write(\"Fees and Attendance UI (Placeholder)\")\n",
        "\"\"\"\n",
        "}\n",
        "\n",
        "for file_path, content in file_contents.items():\n",
        "    with open(file_path, \"w\") as f:\n",
        "        f.write(content.strip())\n",
        "print(\"All files written successfully.\")"
